<p id="notice"><%= notice %></p>

<h1>Course Makers</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Page number</th>
      <th>Page heading</th>
      <th>Page content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_makers.each do |course_maker| %>
      <tr>
        <td><%= course_maker.course %></td>
        <td><%= course_maker.page_number %></td>
        <td><%= course_maker.page_heading %></td>
        <td><%= course_maker.page_content %></td>
        <td><%= link_to 'Show', course_maker %></td>
        <td><%= link_to 'Edit', edit_course_maker_path(course_maker) %></td>
        <td><%= link_to 'Destroy', course_maker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Maker', new_course_maker_path %>
